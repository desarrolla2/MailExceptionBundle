<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" bgcolor="#f6f6f6">
<body style="background: #f6f6f6; padding-top: 20px">
<table style="padding: 10px 20px; margin: 10px auto; border-radius: 5px; border: 1px solid #adadad;
 width: 90%; background: #fff;">
  <tr>
    <td>
      <img
          src="data:image/png;base64,"
          alt=""/>
    </td>
    <td><h1>{{ class }}: {{ message }}</h1></td>
  </tr>
</table>
<table style="padding: 10px 20px;margin: 10px auto; border-radius: 5px; border: 1px solid #adadad; width: 90%;
 background: #fff;">
  <tr>
    <h2>Information</h2>
    <hr/>
    <ol>
      <li>Date: {{ 'now' | date("F jS \\a\\t g:ia") }}</li>
      <li>Path: <a href="{{ host }}{{ path }}"> {{ host }}{{ path }}</a></li>
      <li>User: {{ value_dump(user) }}</li>
    </ol>
  </tr>
</table>
<table style="padding: 10px 20px;margin: 10px auto; border-radius: 5px; border: 1px solid #adadad; width: 90%;
 background: #fff;">
  <tr>
    <h2>Stack Trace</h2>
    <hr/>
    <ol>
      {% for line in trace %}
        <li>{{ line }}</li>
      {% endfor %}
    </ol>
  </tr>
</table>
{% if get|length %}
  <table style="padding: 10px 20px;margin: 10px auto; border-radius: 5px; border: 1px solid #adadad; width: 90%;
 background: #fff;">
    <tr>
      <h2>Get</h2>
      <hr/>
      <ol>
        {% for key, value in get %}
          <li>{{ key }}: {{ value_dump(value) }}</li>
        {% endfor %}
      </ol>
    </tr>
  </table>
{% endif %}
{% if post|length %}
  <table style="padding: 10px 20px;margin: 10px auto; border-radius: 5px; border: 1px solid #adadad; width: 90%;
 background: #fff;">
    <tr>
      <h2>Post</h2>
      <hr/>
      <ol>
        {% for key, value in post %}
          <li>{{ key }}: {{ value_dump(value) }}</li>
        {% endfor %}
      </ol>
    </tr>
  </table>
{% endif %}
{% if session|length %}
  <table style="padding: 10px 20px;margin: 10px auto; border-radius: 5px; border: 1px solid #adadad; width: 90%;
 background: #fff;">
    <tr>
      <h2>Session</h2>
      <hr/>
      <ol>
        {% for key, value in session %}
          <li>{{ key }}: {{ value_dump(value) }}</li>
        {% endfor %}
      </ol>
    </tr>
  </table>
{% endif %}
</body>
</html>
